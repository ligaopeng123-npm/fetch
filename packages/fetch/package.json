{
    "name": "@gaopeng123/fetch",
    "version": "0.3.6",
    "author": "ligaopeng123",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/fetch.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "set NODE_ENV=development&&webpack serve",
        "start": "tsdx watch",
        "build": "tsdx build",
        "push": "npm publish --access public",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ligaopeng123-npm/fetch.git"
    },
    "size-limit": [
        {
            "path": "dist/fetch.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/fetch.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.2",
        "commitizen": "^4.3.0",
        "conventional-changelog-cli": "^2.2.2",
        "cz-conventional-changelog": "^3.3.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.1",
        "lerna": "^6.1.0",
        "size-limit": "^5.0.2",
        "ts-loader": "^9.2.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.0.0"
    },
    "dependencies": {
        "@gaopeng123/utils.file": "latest",
        "@gaopeng123/utils.types": "latest",
        "node-fetch": "^2.6.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "workspaces": [
        "packages/*"
    ]
}
